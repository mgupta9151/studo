<div class="right_col" role="main">
  <div class="">
    <div class="page-title">
      <div class="title_left">
        <h3>Configuraci√≥n</h3>
      </div>              
    </div>
    <div class="clearfix"></div>
    <div class="row">
      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_content">
            <%=form_for [:admins , @configuration],:html => {class: "form-horizontal form-label-left",novalidate: "novalidate"}  do |f|%>
           
            <div class="item form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="key_name">Llave <span class="required">*</span>
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">                         
                <%=f.text_field :key_name,required: "required", class: "form-control col-md-7 col-xs-12",readonly:true%>
              </div>
            </div>

            <div class="item form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="value">Valor <span class="required">*</span>
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">                         
                <%=f.text_field :value,required: "required", class: "form-control col-md-7 col-xs-12",readonly:true%>
              </div>
            </div>
                 
            <div class="ln_solid"></div>
            <div class="form-group">
              <div class="col-md-6 col-md-offset-3">
                <%=link_to "edit",edit_admins_general_configuration_path(@configuration),class: "btn btn-primary"%>
              </div>
            </div>
            <%end%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>